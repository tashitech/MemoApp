{
  "expo": {
    "name": "関西風メモアプリ",
    "slug": "関西風メモアプリ",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/memo_app_icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.example.memoapp",
      "buildNumber": "1.0.0"
    },
    "android": {
      "package": "com.example.memoapp",
      "versionCode": 1,
      "permissions": [],
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router"
    ]
  }
}
